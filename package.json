{
  "name": "pknote-admin-front",
  "private": true,
  "version": "0.1.0",
  "description": "pknote project management backend front-end interface, the project technology stack is: Vue3,vite,typescript,element-plus",
  "author": "toimc-team <admin@wayearn.com>",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "vite",
    "build:emit": "vue-tsc --noEmit && vite build",
    "build": "vite build",
    "serve": "vite preview",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:pretty": "pretty-quick --staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^0.2.7",
    "@tinymce/tinymce-vue": "^4.0.6",
    "element-plus": "2",
    "normalize.css": "^8.0.1",
    "reset-css": "^5.0.1",
    "tinymce": "^5.10.3",
    "vue": "^3.2.31",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/compiler-sfc": "^3.2.31",
    "commitizen": "^4.2.4",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.49.8",
    "scss": "^0.2.4",
    "stylelint": "^14.5.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.5",
    "unplugin-auto-import": "^0.6.0",
    "unplugin-vue-components": "^0.17.18",
    "vite": "^2.8.4",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^0.31.4"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "keywords": [
    "vue",
    "element-plus",
    "vue3",
    "ts",
    "tsx",
    "admin",
    "typescript",
    "toimc"
  ]
}
